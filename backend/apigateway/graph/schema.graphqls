# Sense API
# ---------
type Color {
  r: Int
  g: Int
  b: Int
}

type Orientation {
  pitch: Float,
  roll: Float,
  yaw: Float
}

type Temperature {
  value: Float
}

type Humidity {
  value: Float
}

type Pressure {
  value: Float
}

type Query {
  placeholder: String
}

type Subscription {
  color: Color 
  temperature: Temperature
  humidity: Humidity
  pressure: Pressure
  orientation: Orientation
}

